{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "moduleResolution": "node",
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "jsx": "preserve",
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
    },
    "noImplicitAny": false,
    "types": [
      "@nuxt/types",
      "@nuxtjs/axios",
      "@types/node"
    ],
    "typeRoots": ["./node_modules/@types", "types"]
  },
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist",
  ]
}
